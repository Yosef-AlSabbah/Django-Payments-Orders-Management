# Ignore PyCharm specific files
.idea/
*.iml

# Ignore Python compiled files
__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore virtual environments
venv/
.env/
env/

# Ignore environment variables file
.env

# Ignore database files
*.sqlite3
*.db

# Ignore migrations cache (keep actual migrations)
